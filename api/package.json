{
  "name": "mythos-forge-api",
  "version": "0.0.1",
  "description": "Server for MythosForge",
  "main": "server.js",
  "scripts": {
    "start": "dotenv -e .env.development -- ts-node-dev --transpile-only --no-notify src/server.ts",
    "generate-keys": "ts-node-dev first-run.ts",
    "clean": "rm -rf ./lib",
    "nexus:reflect": "NODE_ENV=\"production\" ts-node-dev --transpile-only ./src/graphql",
    "build": "npm run clean && npm run nexus:reflect && NODE_ENV=\"production\" tsc",
    "prisma-sync": "prisma db push && npx prisma generate",
    "db-migrate": "npx prisma migrate dev",
    "db-deploy": "dotenv -e .env.production prisma migrate deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brokenwire-Apps/mythos-forge.git"
  },
  "author": "JACK-COM",
  "license": "MIT",
  "homepage": "https://github.com/Brokenwire-Apps/mythos-forge#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.321.1",
    "@prisma/client": "^4.14.1",
    "apollo-server": "^3.11.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "crypto-js": "^4.1.1",
    "docx": "^8.0.3",
    "dotenv": "^16.0.3",
    "easy-template-x": "^3.1.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.4.0",
    "graphql": "^16.6.0",
    "graphql-import-node": "^0.0.5",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^2.4.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nexus": "^1.3.0",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-google-oidc": "^0.1.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/luxon": "^2.3.2",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/passport": "^1.0.12",
    "prisma": "^4.14.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
