{
  "name": "prisma-express",
  "version": "0.3.0",
  "description": "BoilerPlate Node TS+Express+Authentication App (with Prisma)",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --transpile-only --no-notify src/server.ts",
    "generate-keys": "ts-node-dev first-run.ts",
    "build": "npm run clean && npm run nexus:reflect && NODE_ENV=\"production\" tsc",
    "clean": "rm -rf ./lib",
    "nexus:reflect": "NODE_ENV=\"production\" ts-node-dev --transpile-only ./src/graphql",
    "prisma-sync": "prisma db push && npx prisma generate",
    "prisma-seed": "npx prisma db seed",
    "db-migrate": "npx prisma migrate dev",
    "bump:patch": "npm version patch && git push && git push --tags",
    "bump:minor": "npm version minor && git push && git push --tags",
    "bump:major": "npm version major && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JACK-COM/prisma-express.git"
  },
  "author": "JACK-COM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JACK-COM/prisma-express/issues"
  },
  "homepage": "https://github.com/JACK-COM/prisma-express#readme",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.321.1",
    "@prisma/client": "^5.1.1",
    "apollo-server": "^3.11.1",
    "argon2": "^0.30.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-rate-limit": "^6.4.0",
    "graphql": "^16.6.0",
    "graphql-import-node": "^0.0.5",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.3.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nexus": "^1.3.0",
    "passport": "^0.6.0",
    "passport-google-oidc": "^0.1.0",
    "passport-local": "^1.0.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/luxon": "^3.3.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prisma": "^5.1.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
