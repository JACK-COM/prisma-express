{
  "name": "prisma-express",
  "version": "0.5.0",
  "description": "BoilerPlate Node TS+Express+Authentication App (with Prisma)",
  "main": "index.js",
  "scripts": {
    "prestart": "dotenv -- ts-node-dev ./scripts/db-migrate.ts",
    "start": "dotenv -- ts-node-dev --transpile-only --no-notify src/server.ts",
    "generate-keys": "ts-node-dev srcipts/first-run.ts",
    "build": "npm run clean && npm run nexus:reflect && NODE_ENV=\"production\" tsc",
    "clean": "rm -rf ./lib",
    "nexus:reflect": "NODE_ENV=\"production\" ts-node-dev --transpile-only ./src/graphql",
    "prisma-sync": "prisma db push && npx prisma generate",
    "prisma-seed": "npx prisma db seed",
    "db-migrate": "npx prisma migrate dev",
    "bump:patch": "npm version patch && git push && git push --tags",
    "bump:minor": "npm version minor && git push && git push --tags",
    "bump:major": "npm version major && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JACK-COM/prisma-express.git"
  },
  "author": "JACK-COM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JACK-COM/prisma-express/issues"
  },
  "homepage": "https://github.com/JACK-COM/prisma-express#readme",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "@aws-sdk/client-s3": "^3.525.0",
    "@prisma/client": "^6.2.1",
    "argon2": "^0.41.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.1.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "graphql": "^16.8.1",
    "graphql-import-node": "^0.0.5",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nexus": "^1.3.0",
    "passport": "^0.7.0",
    "passport-google-oidc": "^0.1.0",
    "passport-local": "^1.0.0",
    "tsconfig-paths": "^4.2.0",
    "uuidv7": "^1.0.2",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cookie-session": "^2.0.48",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/luxon": "^3.4.2",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.10.6",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prisma": "^6.2.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  }
}
